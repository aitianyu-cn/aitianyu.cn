{
    "global": { "param": { "lang": "string" } },
    "server": {
        "generic": {
            "description": "tianyu generic server for global definitions.",
            "port": 9000,
            "dispatcher": {
                "aitianyu/cn/generic/features/feature-toggle": {
                    "param": {
                        "project": "string?"
                    }
                },
                "aitianyu/cn/generic/language/language-alls": {
                    "param": null
                },
                "aitianyu/cn/generic/home/navigation": {
                    "param": null
                }
            }
        },
        "project": {
            "description": "tianyu home page projects server.",
            "port": 9001,
            "dispatcher": {
                "aitianyu/cn/project/all": { "param": null },
                "aitianyu/cn/project/download/all": { "param": null },
                "aitianyu/cn/project/document/all": { "param": null },
                "aitianyu/cn/project/document/macro": {
                    "param": {
                        "project": "string",
                        "showAll": "boolean?",
                        "page": "number?",
                        "size": "number?"
                    }
                },
                "aitianyu/cn/project/document/api": {
                    "param": {
                        "project": "string",
                        "namespace": "string?",
                        "member": "string?"
                    }
                },
                "aitianyu/cn/project/document/help": {
                    "param": {
                        "project": "string"
                    }
                },
                "aitianyu/cn/project/document/arch": {
                    "param": {
                        "project": "string"
                    }
                },
                "aitianyu/cn/project/document/resource": {
                    "param": {
                        "project": "string",
                        "option": "string",
                        "file": "string"
                    }
                }
            }
        },
        "remote": {
            "description": "tianyu local server remote connection service.",
            "port": 9002,
            "dispatcher": {
                "aitianyu/cn/remote/server": {
                    "param": {
                        "action": "string",
                        "server": "string",
                        "token": "string",
                        "template for action": {
                            "update/add": {
                                "connector": {
                                    "network name": {
                                        "ip": "string",
                                        "auth": ["string"]
                                    }
                                }
                            },
                            "remove": {
                                "auth": ["string"],
                                "network name": "string"
                            }
                        }
                    }
                },
                "aitianyu/cn/remote/get-server": {
                    "param": {
                        "auth": "string?",
                        "token": "string",
                        "server": ["string"]
                    }
                }
            }
        },
        "authorization": {
            "description": "tianyu authorization server for all users of tianyu projects, tianyu applications, etc...",
            "port": 9010,
            "dispatcher": {
                "aitianyu/cn/authorization/online/login": {
                    "param": {
                        "user": "string",
                        "pw": "string",
                        "local": "string"
                    }
                },
                "aitianyu/cn/authorization/online/logout": {
                    "param": { "token": "string" }
                },
                "aitianyu/cn/authorization/online/status": {
                    "param": [
                        { "token": "string" },
                        { "user": "string", "local": "string" }
                    ]
                }
            }
        },
        "gitool": {
            "description": "data source for github auto backup, snapshot tool.",
            "port": 9100,
            "dispatcher": {
                "aitianyu/cn/git/tool/helper/source": {
                    "param": {
                        "server": "string",
                        "token": "string"
                    }
                }
            }
        },
        "tpro": {
            "description": "tianyu project management application server.",
            "port": 9110,
            "dispatcher": {}
        },

        "app": {
            "image-selector": {
                "description": "provide selector for images.",
                "port": 9200,
                "dispatcher": {
                    "aitianyu/cn/app/image/selector/createToken": {
                        "param": {
                            "name": "string",
                            "safe": "string"
                        },
                        "return": "string - return the token"
                    },
                    "aitianyu/cn/app/image/selector/releaseToken": {
                        "param": {
                            "token": "string",
                            "safe": "string"
                        },
                        "return": "string"
                    },

                    "aitianyu/cn/app/image/selector/uploader": {
                        "param": {
                            "token": "string",
                            "safe": "string",
                            "images": [
                                {
                                    "name": "string",
                                    "id": "string",
                                    "image": "base64:"
                                }
                            ]
                        },
                        "return": "string - return the status"
                    },
                    "aitianyu/cn/app/image/selector/getter/selected": {
                        "param": {
                            "token": "string"
                        },
                        "return": [
                            {
                                "name": "string",
                                "id": "string"
                            }
                        ]
                    },
                    "aitianyu/cn/app/image/selector/getter/list": {
                        "param": {
                            "token": "string"
                        },
                        "return": [
                            {
                                "valid": "boolean",
                                "all": ["string"],
                                "selected": ["string"]
                            }
                        ]
                    },
                    "aitianyu/cn/app/image/selector/getter/images": {
                        "param": {
                            "token": "string",
                            "images": ["string"]
                        },
                        "return": [
                            {
                                "name": "string",
                                "image": "base64:"
                            }
                        ]
                    },

                    "aitianyu/cn/app/image/selector/select": {
                        "param": {
                            "token": "string",
                            "images": ["string"]
                        }
                    },
                    "aitianyu/cn/app/image/selector/unselect": {
                        "param": {
                            "token": "string",
                            "images": ["string"]
                        }
                    }
                }
            },
            "daily": {
                "description": "provide daily service for key-days, members, and etc.",
                "port": 9300,
                "dispatcher": {
                    "aitianyu/cn/app/daily/infar/generator/author": {
                        "param": {
                            "name": "string",
                            "key": "string"
                        },
                        "return": "string"
                    },
                    "aitianyu/cn/app/daily/infar/generator/subAuthor": {
                        "param": {
                            "author": "string",
                            "name": "string",
                            "key": "string"
                        },
                        "return": "string"
                    },
                    "aitianyu/cn/app/daily/infar/getUser": {
                        "param": {
                            "author": "string",
                            "key": "string"
                        },
                        "return": "string"
                    },

                    "aitianyu/cn/app/daily/timer/record": {
                        "param": {
                            "author": "string",
                            "time": "string",
                            "summary": "string",
                            "description": "string"
                        }
                    },
                    "aitianyu/cn/app/daily/timer/get": {
                        "param": {
                            "author": "string",
                            "summary": "string"
                        },
                        "return": [
                            {
                                "time": "string",
                                "during": "number",
                                "summary": "string",
                                "description": "string"
                            }
                        ]
                    },
                    "aitianyu/cn/app/daily/timer/getAll": {
                        "param": {
                            "author": "string"
                        },
                        "return": [
                            {
                                "time": "string",
                                "during": "number",
                                "summary": "string",
                                "description": "string"
                            }
                        ]
                    }
                }
            }
        }
    }
}

{
    "include": [
        "public",
        "static",
        "resource/core",
        "resource/security",
        "resource/theme",
        "resource/ui",
        "resource/tianyu",
        "tianyu/hooks",
        "tianyu/app",
        "tianyu/i18n",
        "tianyu/shell",
        "resource/global.d.ts"
    ],
    "compilerOptions": {
        "composite": true,
        "target": "es2021",
        "module": "esnext",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "noFallthroughCasesInSwitch": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": false,
        "jsx": "preserve",
        "baseUrl": ".",
        "outDir": "./dist",
        "paths": {
            "ts-public/*": ["public/*"],
            "ts-static/*": ["static/*"],
            "ts-core/*": ["resource/core/*"],
            "ts-sec/*": ["resource/security/*"],
            "ts-theme/*": ["resource/theme/*"],
            "ts-ui/*": ["resource/ui/*"],
            "ts-hooks/*": ["tianyu/hooks/*"],
            "tianyu-app/*": ["tianyu/app/*"],
            "tianyu-i18n/*": ["tianyu/i18n/*"],
            "tianyu-shell/*": ["tianyu/shell/*"]
        }
    }
}
